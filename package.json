{
	"name": "jump-alias-path",
	"displayName": "jumpAliasPath",
	"description": "jumpAliasPath",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"publisher": "liuhe945",
	"keywords": [
		"alias",
		"webpack",
		"别名跳转"
	],
	"categories": [
		"Other"
	],
	"icon": "favicon.ico",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "jumpAliasPath setting",
			"properties": {
				"fileAliasSetting.aliasMap": {
					"type": "object",
					"default": {},
					"scope": "resource",
					"description": "以工作空间顶层文件夹命名的映射表"
				},
				"fileAliasSetting.fileSuffixs": {
					"type": "object",
					"default": {},
					"scope": "resource",
					"description": "需要解析的文件名称"
				},
				"fileAliasSetting.configPos": {
					"type": "object",
					"default": {},
					"scope": "resource",
					"description": "指示webpack config 文件夹相对位置，首推"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:redDragonLH/vscode-jumpAliasPath.git"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
